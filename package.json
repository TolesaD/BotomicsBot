{
  "name": "Botomics",
  "version": "1.0.0",
  "description": "A Telegram bot that allows users to create and manage their own mini-bots with Botomics Wallet System",
  "main": "server.js",
  "scripts": {
    "start": "cross-env NODE_ENV=production node server.js",
    "start:dev": "cross-env NODE_ENV=development node server.js",
    "start:prod": "cross-env NODE_ENV=production node server.js",
    "wallet": "cross-env NODE_ENV=production node server.js",
    "wallet:dev": "cross-env NODE_ENV=development PORT=3001 node server.js",
    "wallet:prod": "cross-env NODE_ENV=production PORT=3000 node server.js",
    "both": "concurrently \"npm run bot\" \"npm run wallet:dev\"",
    "bot": "cross-env NODE_ENV=production node src/app.js",
    "bot:dev": "cross-env NODE_ENV=development node src/app.js",
    "bot:prod": "cross-env NODE_ENV=production node src/app.js",
    "server": "cross-env NODE_ENV=production node server.js",
    "server:dev": "cross-env NODE_ENV=development node server.js",
    "server:prod": "cross-env NODE_ENV=production node server.js",
    "test": "cross-env NODE_ENV=test node test-local.js",
    "test:dev": "cross-env NODE_ENV=development node test-local.js",
    "test:prod": "cross-env NODE_ENV=production node test-local.js",
    "test:quick": "node test-quick.js",
    "setup": "cross-env NODE_ENV=development node setup.js",
    "setup:dev": "cross-env NODE_ENV=development node setup.js",
    "setup:prod": "cross-env NODE_ENV=production node setup.js",
    "cpanel": "cross-env NODE_ENV=production node cpanel-start.js",
    "cpanel:start": "cross-env NODE_ENV=production node cpanel-start.js",
    "health": "cross-env NODE_ENV=production node health.js",
    "health:dev": "cross-env NODE_ENV=development node health.js",
    "health:prod": "cross-env NODE_ENV=production node health.js",
    "env:dev": "cross-env NODE_ENV=development node scripts/switch-env.js development",
    "env:prod": "cross-env NODE_ENV=production node scripts/switch-env.js production",
    "migrate": "cross-env NODE_ENV=development node scripts/ensureDatabase.js",
    "migrate:advanced": "cross-env NODE_ENV=development node scripts/create_advanced_tables.js",
    "migrate:all": "npm run migrate:advanced && npm run migrate:new-features",
    "migrate:new-features": "cross-env NODE_ENV=development node scripts/create_new_features_tables.js",
    "dev": "cross-env NODE_ENV=development nodemon server.js",
    "dev:all": "concurrently \"npm run bot:dev\" \"npm run server:dev\"",
    "dev:wallet": "cross-env NODE_ENV=development nodemon server.js",
    "test:new-features": "cross-env NODE_ENV=development node test-new-features.js",
    "migrate:subscription": "cross-env NODE_ENV=development node src/scripts/create_subscription_tables.js",
    "setup:complete": "npm run migrate:subscription && cross-env NODE_ENV=production node server.js",
    "setup:botomics": "cross-env NODE_ENV=development node src/scripts/setup_botomics.js",
    "deploy": "npm run migrate:all && npm run setup:botomics && npm run set-commands",
    "deploy:full": "npm run migrate:all && npm run setup:botomics && npm run set-commands && npm run server:prod",
    "set-commands": "cross-env NODE_ENV=production node scripts/setCommands.js",
    "set-commands:minibots": "cross-env NODE_ENV=production node scripts/setMiniBotCommands.js",
    "cron:daily": "cross-env NODE_ENV=production node src/scripts/daily_maintenance.js",
    "cron:subscriptions": "cross-env NODE_ENV=production node src/scripts/process_subscriptions.js",
    "wallet:setup": "cross-env NODE_ENV=development node src/scripts/create_botomics_tables.js",
    "wallet:test": "cross-env NODE_ENV=development node src/scripts/test_botomics.js",
    "wallet:migrate": "npm run migrate:subscription && npm run wallet:setup",
    "debug:minibots": "cross-env NODE_ENV=development node debugMiniBots.js",
    "debug:env": "cross-env NODE_ENV=development node debug-env.js",
    "debug:wallet": "cross-env NODE_ENV=development node debug-wallet.js",
    "backup": "cross-env NODE_ENV=production node scripts/backup_database.js",
    "restore": "cross-env NODE_ENV=production node scripts/restore_database.js",
    "lint": "eslint . --ext .js",
    "format": "prettier --write .",
    "clean": "rm -rf node_modules package-lock.json && npm install",
    "reset": "npm run clean && npm run setup"
  },
  "keywords": [
    "telegram",
    "bot",
    "wallet",
    "cryptocurrency",
    "botomics",
    "digital-currency",
    "subscription",
    "premium"
  ],
  "author": "Botomics Team",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.2",
    "compression": "^1.7.4",
    "concurrently": "^8.2.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "fs-extra": "^11.2.0",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "moment": "^2.29.4",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^3.0.3",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.35.2",
    "sqlite3": "^5.1.6",
    "telegraf": "^4.16.3",
    "uuid": "^9.0.1",
    "validator": "^13.11.0",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/botomics.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/botomics/issues"
  },
  "homepage": "https://botomics.com"
}
